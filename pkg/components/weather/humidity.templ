package weather

import "github.com/andythigpen/clock2/pkg/models/view"

templ WeatherHumidity(model view.WeatherHumidityView) {
	<div class="flex-1 flex flex-row justify-center items-center">
		<img src={ model.Icon } class="h-96"/>
		<span class="text-[15rem]">{ model.Percentage }</span>
	</div>
}
